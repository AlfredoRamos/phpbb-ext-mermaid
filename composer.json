{
	"name": "alfredoramos/mermaid",
	"type": "phpbb-extension",
	"description": "Mermaid extension for phpBB",
	"homepage": "https://github.com/AlfredoRamos/phpbb-ext-mermaid",
	"version": "0.1.0-dev",
	"time": "2019-07-24",
	"keywords": [
		"phpbb",
		"extension",
		"mermaid",
		"diagrams",
		"flowchart",
		"sequence-diagram",
		"gantt-diagram",
		"class-diagram",
		"state-diagram",
		"git-graph",
		"pie-chart"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Alfredo Ramos",
			"email": "alfredo.ramos@yandex.com",
			"homepage": "https://github.com/AlfredoRamos",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": ">=5.6",
		"composer/installers": "^1.6.0"
	},
	"require-dev": {
		"phing/phing": "^2.16.0",
		"pear/archive_tar": "^1.4.0"
	},
	"suggest": {
		"alfredoramos/markdown": "Form Markdown support"
	},
	"extra": {
		"display-name": "Mermaid",
		"soft-require": {
			"phpbb/phpbb": "~3.2.0"
		},
		"version-check": {
			"host": "alfredoramos.github.io",
			"directory": "/phpbb-extensions/versions",
			"filename": "mermaid.json",
			"ssl": true
		}
	}
}
